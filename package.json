{
  "version": "0.1.0",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "strapi5",
    "rate-limit",
    "rate-limiting",
    "throttle",
    "redis",
    "api-security"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "ioredis": "^5.9.2",
    "ms": "^2.1.3",
    "picomatch": "^4.0.3",
    "rate-limiter-flexible": "^9.1.1",
    "react-intl": "^8.1.3"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.35.0",
    "@strapi/sdk-plugin": "^5.4.0",
    "@types/ms": "^2.1.0",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@strapi/typescript-utils": "^5.35.0",
    "prettier": "^3.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.35.0",
    "@strapi/sdk-plugin": "^5.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.9"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-rate-limit",
    "displayName": "Strapi Rate Limiter",
    "description": "Production-ready rate limiting for Strapi 5 — IP-based, identity-aware, with optional Redis backing"
  },
  "name": "strapi-plugin-rate-limit",
  "description": "Production-ready rate limiting for Strapi 5 — IP-based, identity-aware, with optional Redis backing",
  "license": "MIT",
  "author": "bolg55 <kellen.bolger@strapi.io>"
}
